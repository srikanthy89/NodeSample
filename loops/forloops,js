// We use loops to travel/traverse through the array or object

const arr = [1,2,3,4,5,21,34,14];
for (let i =0; i < arr.length; i++) {
    // console.log(arr[i]);
}

for (let i =arr.length-1; i >= 0; i--) { //for ([initialExpression]; [conditionExpression]; [incrementExpression])
    //console.log(i); // it gives all values that is >=0 like 76543210
     console.log(arr[i]);
}